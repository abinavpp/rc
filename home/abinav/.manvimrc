runtime! archlinux.vim
let mapleader=' '
map q ZQ
map <A-z> <C-z>
map <Leader>h :noh<CR>
map <Leader>w :set wrap!<CR>
set mouse=a
set incsearch
set hlsearch
set ignorecase
set smartcase
set noswapfile
set scrolloff=44
so ~/.vim/bundle/vim-searchindex/plugin/searchindex.vim
map <silent> c :call cursor(22,0)<CR>
